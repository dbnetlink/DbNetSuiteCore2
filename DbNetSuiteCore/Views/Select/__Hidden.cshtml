@model DbNetSuiteCore.Models.SelectModel
@inject IConfiguration Configuration
@using Microsoft.Extensions.Configuration
@using Newtonsoft.Json
@using DbNetSuiteCore.Helpers
@using DbNetSuiteCore.Constants
<input type="hidden" name="model" value="@TextHelper.ObfuscateString(JsonConvert.SerializeObject(Model), Configuration)" />
@if (Model.IsParent)
{
    <input type="hidden" name="summarymodel" value="@TextHelper.ObfuscateString(JsonConvert.SerializeObject(Model.SummaryModel), Configuration)" />
}
@if (string.IsNullOrEmpty(Model.JSON) == false)
{
    <input type="hidden" name="json" value="@TextHelper.Compress(Model.JSON)" />
}