<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>DbNetSuiteCore</id>
    <version>2.0.35</version>
    <authors>DbNetLink</authors>
    <owners>DbNetLink</owners>
    <icon>nuget_logo.png</icon>
    <readme>readme.md</readme>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <licenseUrl>https://licenses.nuget.org/MIT</licenseUrl>
    <projectUrl>https://dbnetsuitecore.com/</projectUrl>
    <description>Interactive data-driven UI components for ASP.Net Core Razor pages and MVC views. Support for MSSQL, MySql, PostgreSql, MongoDB, SQLite, JSON, Excel, CSV and the file system</description>
    <copyright>Copyright (c) DbNetLink Limited 2024</copyright>
    <tags>asp.netcore components component asp.net core razor blazor htmx ssr spreadsheet excel csv sqlite mysql mssql postgresql mongodb json datagrid datatable</tags>

    <!-- Dependencies common to both frameworks -->
    
    <dependencies>
      <group targetFramework="net7.0">
        <dependency id="ClosedXML" version="0.104.1" exclude="Build,Analyzers" />
        <dependency id="CsvHelper" version="30.0.1" exclude="Build,Analyzers" />
        <dependency id="Microsoft.AspNetCore.Mvc.Razor.RuntimeCompilation" version="7.0.15" exclude="Build,Analyzers" />
        <dependency id="Microsoft.AspNetCore.WebUtilities" version="2.1.1" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Data.SqlClient" version="5.1.4" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Data.Sqlite" version="7.0.15" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Data.Sqlite.Core" version="7.0.15" exclude="Build,Analyzers" />
        <dependency id="MongoDB.Driver" version="2.23.1" exclude="Build,Analyzers" />
        <dependency id="Newtonsoft.Json" version="13.0.3" exclude="Build,Analyzers" />
        <dependency id="RazorEngineCore" version="2022.1.2" exclude="Build,Analyzers" />
        <dependency id="System.Data.OleDb" version="7.0.0" exclude="Build,Analyzers" />
      </group>

      <group targetFramework="net8.0">
        <dependency id="ClosedXML" version="0.104.1" exclude="Build,Analyzers" />
        <dependency id="CsvHelper" version="33.0.1" exclude="Build,Analyzers" />
        <dependency id="Microsoft.AspNetCore.Mvc.Razor.RuntimeCompilation" version="8.0.10" exclude="Build,Analyzers" />
        <dependency id="Microsoft.AspNetCore.WebUtilities" version="8.0.10" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Data.SqlClient" version="5.2.2" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Data.Sqlite" version="8.0.10" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Data.Sqlite.Core" version="8.0.10" exclude="Build,Analyzers" />
        <dependency id="MongoDB.Driver" version="2.30.0" exclude="Build,Analyzers" />
        <dependency id="Newtonsoft.Json" version="13.0.3" exclude="Build,Analyzers" />
        <dependency id="RazorEngineCore" version="2024.4.1" exclude="Build,Analyzers" />
        <dependency id="System.Data.OleDb" version="8.0.1" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>

  <files>

    <!-- Documentation -->
    <file src="README.md" target="\readme.md" />
    <file src="nuget_logo.png" target="\nuget_logo.png" />

    <!-- Assembly files for .NET 7 -->
    <file src="..\bin\Release\net7.0\DbNetSuiteCore.dll" target="lib\net7.0" />

    <!-- Assembly files for .NET 8 -->
    <file src="..\bin\Release\net8.0\DbNetSuiteCore.dll" target="lib\net8.0" />

  </files>
</package>

