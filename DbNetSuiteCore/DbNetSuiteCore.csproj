    <EmbeddedResource Include="Resources\SVG\spinner.svg" />
    <EmbeddedResource Include="TypeScript\gridcontrol.ts" />
    <EmbeddedResource Include="Views\GridToolbar.cshtml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Views\GridRows.cshtml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Views\_dropDownLookup.cshtml">
