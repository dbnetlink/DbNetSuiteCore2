@model DbNetTimeCore.Pages.TimestreamModel
@{
    Layout = "_Layout";
}

<div role="tablist" class="tabs tabs-lifted m-5  tabs-lg">
    <input type="radio" name="my_tabs_2" role="tab" class="tab" aria-label="MT2" checked />
    <div role="tabpanel" class="tab-content bg-base-100 border-base-300 rounded-box p-6">
        @await Html.PartialAsync("~/Pages/Shared/_gridControl.cshtml", Model.MT2Grid)
    </div>

    <input type="radio" name="my_tabs_2" role="tab" class="tab" aria-label="MT1" />
    <div role="tabpanel" class="tab-content bg-base-100 border-base-300 rounded-box p-6">
        @await Html.PartialAsync("~/Pages/Shared/_gridControl.cshtml", Model.MT1Grid)
    </div>
</div>

@await Html.PartialAsync("~/Pages/Shared/_gridScript.cshtml")


@section Styles {
    @await Html.PartialAsync("~/Pages/Shared/_gridStyle.cshtml")
}