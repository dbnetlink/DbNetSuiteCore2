<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <OutputType>Library</OutputType>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Pages\Shared\**" />
    <Content Remove="Pages\Shared\**" />
    <EmbeddedResource Remove="Pages\Shared\**" />
    <None Remove="Pages\Shared\**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="Models\DataColumn.cs" />
    <Compile Remove="Models\DataRow.cs" />
    <Compile Remove="Services\AspNetCoreServices.cs" />
  </ItemGroup>

  <ItemGroup>
    <Content Remove="Pages\index.cshtml" />
    <Content Remove="Pages\timestream.cshtml" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Resources\CSS\bootstrap.min.css" />
    <None Remove="Resources\CSS\daisyui.css" />
    <None Remove="Resources\CSS\gridcontrol.css" />
    <None Remove="Resources\GIF\spinner.gif" />
    <None Remove="Resources\ICO\favicon.ico" />
    <None Remove="Resources\JS\bootstrap.min.js" />
    <None Remove="Resources\JS\gridcontrol.js" />
    <None Remove="Resources\JS\htmx.min.js" />
    <None Remove="Resources\JS\jquery.min.js" />
    <None Remove="Resources\JS\surreal.js" />
    <None Remove="Resources\JS\tailwindcss.js" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Resources\CSS\gridcontrol.css" />
    <EmbeddedResource Include="Resources\GIF\spinner.gif" />
    <EmbeddedResource Include="Resources\JS\gridcontrol.js" />
    <EmbeddedResource Include="Views\_dropDownLookup.cshtml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Views\_formMarkup.cshtml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Views\_gridControl.cshtml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Views\_gridControlForm.cshtml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Views\_gridMarkup.cshtml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Views\_gridScript.cshtml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Views\_gridStyle.cshtml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Pages\timestream.cshtml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Pages\index.cshtml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Resources\CSS\bootstrap.min.css" />
    <EmbeddedResource Include="Resources\CSS\daisyui.css" />
    <EmbeddedResource Include="Resources\ICO\favicon.ico" />
    <EmbeddedResource Include="Resources\JS\bootstrap.min.js" />
    <EmbeddedResource Include="Resources\JS\htmx.min.js" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Resources\JS\jquery.min.js" />
    <EmbeddedResource Include="Resources\JS\surreal.js" />
    <EmbeddedResource Include="Resources\JS\tailwindcss.js" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AWSSDK.TimestreamQuery" Version="3.7.301.35" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Razor.RuntimeCompilation" Version="8.0.5" />
    <PackageReference Include="Microsoft.AspNetCore.WebUtilities" Version="8.0.6" />
    <PackageReference Include="Microsoft.Data.SqlClient" Version="5.2.1" />
    <PackageReference Include="Microsoft.Data.Sqlite" Version="8.0.6" />
    <PackageReference Include="Microsoft.Data.Sqlite.Core" Version="8.0.6" />
    <PackageReference Include="RazorEngineCore" Version="2024.4.1" />
  </ItemGroup>

</Project>
