@page
@using DbNetSuiteCore.Enums
@using DbNetSuiteCore.Web.Enums
@using DbNetSuiteCore.Helpers
@using DbNetSuiteCore.Models
@using Microsoft.Extensions.Primitives;
@using DbNetSuiteCore.Web.Helpers
@{
    var customersForm = new FormModel(DataSourceType.PostgreSql, ConnectionStringHelper.TestConnectionString(Request.Query["db"], DataSourceType.PostgreSql), "customer") { Insert = true, Delete = true};
    @(await new DbNetSuiteCore.Control(HttpContext).Render(customersForm))
}