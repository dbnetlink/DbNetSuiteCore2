@page
@using DbNetSuiteCore.Enums
@using DbNetSuiteCore.Web.Enums
@using DbNetSuiteCore.Helpers
@using DbNetSuiteCore.Models
@using Microsoft.Extensions.Primitives
@using DbNetSuiteCore.Web.Helpers
@{
    var customersForm = new FormModel(DataSourceType.MySql, ConnectionStringHelper.TestConnectionString(Request.Query["db"], DataSourceType.MySql), "Customer") { Insert = true, Delete = true};
    @(await DbNetSuiteCore.Control.Create(HttpContext).Render(customersForm))
}