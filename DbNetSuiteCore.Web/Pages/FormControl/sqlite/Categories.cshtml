@page
@using DbNetSuiteCore.Enums
@using DbNetSuiteCore.Web.Helpers
@using DbNetSuiteCore.Models
@using DbNetSuiteCore.Web.Models

@{
    var customerForm = new FormModel(DataSourceType.SQLite, ConnectionStringHelper.TestConnectionString(Request.Query["db"], DataSourceType.SQLite), "Categories") { Insert = true, Delete = true };


    @(await DbNetSuiteCore.Control.Create(HttpContext).Render(customerForm))
}

